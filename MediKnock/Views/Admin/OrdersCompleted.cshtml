@model MediKnock.Models.Client.UserProfileBE

@{
    ViewBag.Title = "OrdersCompleted";
    Layout = "~/Views/Shared/_AdminMaster.cshtml";
}


<div class="col-xs-9">
    <h2>List<strong> Order History</strong></h2>
</div>



@{
    if (Model.lstCartItem != null && Model.lstCartItem.Count > 0)
    {
        <body>
            <div class="row">

               

                <div class="col-lg-12 portlets">
                    <div class="panel">
                        <div class="panel-content pagination2 table-responsive">
                            <table class="table table-hover table-dynamic">
                                <thead>
                                    <tr>
                                        <th> Product</th>
                                        <th>Quantity</th>
                                        <th>Single</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
        if (Model.lstCartItem != null)
        {
            foreach (var item in Model.lstCartItem)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.ProductName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quantity)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Price)


                    </td>
                    
                   
                </tr>
            }
        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>


            </div>
        </body>
    }
    else
    {
        <h2 style="color:green;float:left; margin:15px;">No Stock Available </h2>
    }
}
